#include<stdio.h>
int main()
{
    int a,b,c;
    printf("enter the three numbers");
    scanf("%d %d %d",&a,&b,&c);
    if(a<b && a<c)
    {
        printf("smaller number is %d",a);
    }
    else if(b<a && b<c)
    {
       printf("smaller number is %d",b);  
    }
    else{
        printf("smaller number is %d",c);
    }
    return 0;
}
